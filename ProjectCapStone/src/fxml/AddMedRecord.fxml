<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTimePicker?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="471.0" prefWidth="552.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AddMedRecordController">
   <children>
      <VBox alignment="CENTER" layoutY="-3.0" prefHeight="455.0" prefWidth="546.0">
         <children>
            <Label alignment="CENTER" prefHeight="64.0" prefWidth="588.0" text="Add Medication Record">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <HBox alignment="CENTER" prefHeight="61.0" prefWidth="588.0">
               <children>
                  <Label prefHeight="27.0" prefWidth="69.0" text="Date:" />
                  <DatePicker fx:id="datePicker_MedDate" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="60.0" prefWidth="588.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="62.0" text="Time:" />
                  <JFXTimePicker fx:id="timePicker_Time" defaultColor="BLACK" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="67.0" prefWidth="588.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="86.0" text="Medication:" />
                  <TextField fx:id="textField_Medication" prefHeight="25.0" prefWidth="211.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="53.0" prefWidth="588.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="72.0" text="Dosage:" />
                  <TextField fx:id="textField_Dosage" prefHeight="25.0" prefWidth="194.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="81.0" prefWidth="588.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="82.0" text="Frequency:" />
                  <TextField fx:id="textField_Frequency" prefHeight="25.0" prefWidth="208.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="78.0" prefWidth="588.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="72.0" text="Notes:" />
                  <TextField fx:id="textField_Notes" prefHeight="25.0" prefWidth="205.0" />
               </children>
            </HBox>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#saveMedRecord" text="Save" />
         </children>
      </VBox>
   </children>
</AnchorPane>
